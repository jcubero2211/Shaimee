{
  "name": "shaymee-ai-agent",
  "version": "1.0.0",
  "description": "AI Agent for Shaymee store integrating WhatsApp Business, Temu API, and Correos de Costa Rica",
  "main": "backend/server.js",
  "scripts": {
    "dev": "concurrently \"npm run backend:dev\" \"npm run ai-agent:dev\" \"npm run dashboard:dev\"",
    "backend:dev": "nodemon backend/server.js",
    "ai-agent:dev": "cd ai-agent && python -m uvicorn main:app --reload --port 8001",
    "dashboard:dev": "cd dashboard && npm run dev",
    "install:all": "npm install && cd dashboard && npm install && cd ../ai-agent && pip install -r requirements.txt",
    "db:migrate": "npx prisma migrate dev",
    "db:generate": "npx prisma generate",
    "build": "npm run build:backend && npm run build:dashboard",
    "build:backend": "echo 'Backend built'",
    "build:dashboard": "cd dashboard && npm run build",
    "start": "node backend/server.js",
    "test": "jest",
    "lint": "eslint .",
    "format": "prettier --write ."
  },
  "keywords": [
    "ai",
    "whatsapp",
    "ecommerce",
    "temu",
    "correos-costa-rica",
    "sinpe",
    "openai"
  ],
  "author": "Shaymee Team",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "helmet": "^7.1.0",
    "morgan": "^1.10.0",
    "dotenv": "^16.3.1",
    "axios": "^1.6.0",
    "jsonwebtoken": "^9.0.2",
    "bcryptjs": "^2.4.3",
    "multer": "^1.4.5-lts.1",
    "sharp": "^0.32.6",
    "redis": "^4.6.10",
    "bull": "^4.12.0",
    "socket.io": "^4.7.4",
    "prisma": "^5.6.0",
    "@prisma/client": "^5.6.0",
    "joi": "^17.11.0",
    "winston": "^3.11.0",
    "compression": "^1.7.4",
    "rate-limiter-flexible": "^3.0.8",
    "express-rate-limit": "^7.1.5",
    "express-validator": "^7.0.1",
    "nodemailer": "^6.9.7",
    "qrcode": "^1.5.3",
    "openai": "^4.20.1",
    "langchain": "^0.0.200",
    "pinecone-client": "^1.1.2",
    "weaviate-ts-client": "^1.4.0",
    "whatsapp-web.js": "^1.23.0",
    "venom-bot": "^5.0.21",
